<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sdbiosensor.covicatch">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/launchericonround"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/launchericonround"
        android:supportsRtl="true"
        android:theme="@style/Theme.Covid_form">
        <activity android:name="com.sdbiosensor.form.TimerActivity"></activity>
        <activity android:name="com.sdbiosensor.form.InstructionActivity" />
        <activity android:name="com.sdbiosensor.AgreementActivity" />
        <activity android:name="com.sdbiosensor.form.CovidFormActivity1" />
        <activity android:name="com.sdbiosensor.form.CovidFormActivity" />
        <activity android:name="com.sdbiosensor.SelectLanguageActivity" />
        <activity android:name="com.sdbiosensor.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>